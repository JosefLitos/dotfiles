# Desktop looks
exec $screen
exec picom --experimental-backends

# Background daemons
exec /usr/lib/polkit-gnome/polkit-gnome-authentication-agent-1
exec dunst
exec redshift -l 50:14.5
# exec --no-startup-id nm-applet

# One time changes
exec acpi | grep D && which cpupower-gui && cpupower-gui pr Powersave
exec_always xset r rate 225 40 && xmodmap -e "clear lock" -e "keycode 0x42 = Escape" && xset s 300
exec_always xrandr -r 50
